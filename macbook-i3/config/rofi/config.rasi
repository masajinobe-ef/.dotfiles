configuration {
    show-icons: false;
    terminal: "alacritty";
    hide-scrollbar: true;
    sidebar-mode: false;

    font: "JetBrainsMono Nerd Font 12";
    lines: 15;
    width: 100%;
    padding: 0;
    location: 0;
    yoffset: 0;
    xoffset: 0;
    border-radius: 0;
    horizontal: false;
    anchor: "north west";
    override-width: true;
    click-to-exit: true;

    matching: "prefix";
    tokenize: false;
    kb-mode-complete: "";
}
@theme "theme.rasi"
